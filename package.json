{
    "name": "schematique-vuetify",
    "version": "0.1.2",
    "description": "A VueJS plugin that allows rendering Vuetify interfaces using JSON schemas",
    "author": {
        "name": "Cosmin Stoian",
        "email": "cosmin@bceng.tech"
    },
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "build-library": "vue-cli-service build --target lib --name schematique-vuetify ./src/install.js"
    },
    "main": "./dist/schematique-vuetify.common.js",
    "files": [
        "dist/*"
    ],
    "dependencies": {
        "core-js": "^3.6.5",
        "luxon": "^2.2.0",
        "vue": "^2.6.11",
        "vue-router": "^3.2.0",
        "vuetify": "^2.4.0"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-eslint": "~4.5.0",
        "@vue/cli-plugin-router": "~4.5.0",
        "@vue/cli-service": "~4.5.0",
        "@vue/eslint-config-airbnb": "^5.3.0",
        "babel-eslint": "^10.1.0",
        "eslint": "^6.7.2",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-vue": "^6.2.2",
        "sass": "~1.32.0",
        "sass-loader": "^10.0.0",
        "vue-cli-plugin-vuetify": "~2.4.5",
        "vue-template-compiler": "^2.6.11",
        "vuetify-loader": "^1.7.0"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "@vue/airbnb",
            "eslint:recommended"
        ],
        "parserOptions": {
            "parser": "babel-eslint"
        },
        "rules": {
            "no-console": "off"
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ],
    "bugs": "https://github.com/cosminstn/schematique-vuetify/issues",
    "keywords": [
        "vue",
        "vuetify",
        "json",
        "forms",
        "vuetify"
    ],
    "license": "MIT",
    "repository": "https://github.com/cosminstn/schematique-vuetify"
}
